datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Play {
  id          Int      @id @default(autoincrement())
  title       String
  description String?
  imageUrl    String?
  videoUrl    String?
}

model landingpageimg {
  id    Int    @id @default(autoincrement())
  url   String
  name  String
}
