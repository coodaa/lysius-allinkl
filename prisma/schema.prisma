datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Play {
  id                       Int      @id @default(autoincrement())
  title                    String
  description              String?
  imageUrl                 String?
  videoUrl                 String?
  subtitle                 String?
  productionDirector       String?
  artisticSupervision      String?
  musicalDirector          String?
  regie                    String?
  produktion               String?
  kuenstlerischeBegleitung String?
  musikalischeLeitung      String?
  mit                      String?
  sopranistin              String?
  sopranist                String?
  bass                     String?
  chor                     String?
  orchester                String?
  foerderung               String?
}



model landingpageimg {
  id    Int    @id @default(autoincrement())
  url   String
  name  String
}
